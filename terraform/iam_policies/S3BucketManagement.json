{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "S3BucketManagement",
            "Effect": "Allow",
            "Action": [
                "s3:DeleteObjectVersion",
                "s3:*Bucket*",
                "s3:*Configuration*",
                "s3:DeleteObject",
                "s3:Describe*",
                "s3:*Tags*"
            ],
            "Resource": "*"
        }
    ]
}