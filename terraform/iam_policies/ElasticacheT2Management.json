{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ElasticacheT2Management",
      "Effect": "Allow",
      "Resource": "*",
      "Action": [
        "elasticache:*"
      ],
      "Condition": {
        "StringLikeIfExists": {
          "elasticache:CacheNodeType": "cache.t2.*"
        }
      }
    }
  ]
}